<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/white"
              android:orientation="vertical"
              tools:context=".a_stock.activity.MultiCreditTradeActivity">

    <include layout="@layout/include_multi_credit"/>

    <com.thinkive.android.trade_bz.views.NavigatorView
        android:id="@+id/nsv_multi_trade"
        android:layout_width="match_parent"
        android:layout_height="37dp"
        android:background="@color/trade_normal_background"/>

    <com.thinkive.android.trade_bz.views.HorizontalSlideLinearLayout
        android:id="@+id/hsll_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/nsv_multi_trade"/>


</LinearLayout>