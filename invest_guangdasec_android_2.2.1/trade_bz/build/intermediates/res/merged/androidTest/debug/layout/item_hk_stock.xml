<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/trade_color_white"
    android:orientation="vertical">

    <View android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/trade_normal_background"/>

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_stock_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:hint="@string/no_text_set"
            android:paddingBottom="2dp"
            android:paddingTop="4dp"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_title"
            />
        <TextView
            android:id="@+id/tv_stock_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="7dp"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:paddingBottom="2dp"
            android:paddingTop="4dp"
            android:textSize="@dimen/hk_select_title"
            />

    </LinearLayout>

    <View android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/trade_divide_line"/>

    <!--第一栏-->
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action0"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:id="@+id/tv_cost_price"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action1"
            android:textSize="@dimen/hk_select_item"/>
        <TextView android:id="@+id/tv_last_price"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>
    </LinearLayout>
    <!--第一栏-->
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action2"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:id="@+id/tv_market_value"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action3"
            android:textSize="@dimen/hk_select_item"/>
        <TextView android:id="@+id/tv_hold_amount"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>
    </LinearLayout>
    <!--第一栏-->
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action4"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:id="@+id/tv_enable_amount"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>


        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action5"
            android:textSize="@dimen/hk_select_item"/>
        <TextView android:id="@+id/tv_freeze_amount"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>
    </LinearLayout>
    <!--第一栏-->
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action6"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:id="@+id/tv_float_yk_per"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>


        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action7"
            android:textSize="@dimen/hk_select_item"/>
        <TextView android:id="@+id/tv_float_yk"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>
    </LinearLayout>

    <!--第一栏-->
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action9"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:id="@+id/tv_on_way_in"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>


        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_stock_action10"
            android:textSize="@dimen/hk_select_item"/>
        <TextView android:id="@+id/tv_on_way_out"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>
    </LinearLayout>

    <!--第一栏-->
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/hk_select_height"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.5"
            android:text="@string/hk_stock_action8"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:id="@+id/tv_money_type_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/hk_select_height"
            android:layout_marginRight="10dp"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="2.1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>

    </LinearLayout>


</LinearLayout>