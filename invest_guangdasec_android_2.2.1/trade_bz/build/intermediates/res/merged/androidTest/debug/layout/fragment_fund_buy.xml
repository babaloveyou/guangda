            android:layout_weight="1"
            android:drawableTop="@drawable/icon22"
            android:gravity="center"
            android:paddingTop="15dp"
            android:paddingBottom="5dp"
            android:text="@string/fund_buy_five"
            android:textSize="@dimen/fund_buy_text"
            android:textColor="@color/trade_text_color6"
            android:visibility="gone"/>

        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/trade_divide_line"
            android:visibility="gone"/>

        <TextView
            android:id="@+id/tv_out_fund_set"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:drawableTop="@drawable/icon23"
            android:gravity="center"
            android:paddingTop="15dp"
            android:paddingBottom="5dp"
            android:text="@string/fund_buy_six"
            android:textSize="@dimen/fund_buy_text"
            android:textColor="@color/trade_text_color6"/>

        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/trade_divide_line"/>

        <TextView
            android:id="@+id/tv_out_fund_account"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:drawableTop="@drawable/icon24"
            android:gravity="center"
            android:paddingTop="15dp"
            android:paddingBottom="5dp"
            android:text="@string/fund_buy_seven"
            android:textSize="@dimen/fund_buy_text"
            android:textColor="@color/trade_text_color6"/>

        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/trade_divide_line"/>

        <TextView
            android:id="@+id/tv_out_fund_risk"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:drawableTop="@drawable/icon25"
            android:gravity="center"
            android:paddingTop="15dp"
            android:paddingBottom="5dp"
            android:text="@string/fund_buy_eight"
            android:textSize="@dimen/fund_buy_text"
            android:textColor="@color/trade_text_color6"/>

        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/trade_divide_line"
            android:visibility="gone"/>

        <TextView
            android:id="@+id/tv_out_fund_direction"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:drawableTop="@drawable/icon28"
            android:gravity="center"
            android:paddingTop="15dp"
            android:paddingBottom="5dp"
            android:text="@string/fund_buy_night"
            android:textSize="@dimen/fund_buy_text"
            android:textColor="@color/trade_text_color6"
            android:background="@color/trade_color_white"
            android:visibility="gone"/>

        <View android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/trade_divide_line"/>

        <TextView
            android:id="@+id/tv_out_fund_remove"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:drawableTop="@drawable/icon21"
            android:gravity="center"
            android:paddingTop="15dp"
            android:paddingBottom="5dp"
            android:text="@string/fund_buy_ten"
            android:textSize="@dimen/fund_buy_text"
            android:textColor="@color/trade_text_color6"
            android:background="@color/trade_color_white"/>
    </LinearLayout>

