<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/trade_color_white">

    <View android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/trade_normal_background"/>

    <View android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/trade_divide_line"/>

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/number_winning_title"
            android:layout_weight="2"
            android:textColor="@color/trade_color1"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:paddingLeft="10dp"
            android:id="@+id/tv_select_entrust"/>

        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:paddingRight="10dp"
            android:layout_weight="0"
            android:id="@+id/tv_winning_data"/>

    </LinearLayout>

    <View android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/trade_divide_line"/>

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_winning_securit_name"
            android:hint="@string/no_text_set"
            android:textStyle="bold"
            android:textColor="@color/trade_color_black"
            android:paddingLeft="10dp"
            android:paddingTop="5dp"
            android:paddingBottom="1dp"
            android:layout_weight="0"/>

        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_winning_securit_number"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:paddingLeft="10dp"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="1"/>

        <TextView android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/number_winning_set"
            android:textColor="@color/trade_color1"
            android:layout_marginRight="10dp"
            android:paddingRight="10dp"
            android:paddingLeft="10dp"
            android:paddingTop="3dp"
            android:paddingBottom="3dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="2dp"
            android:id="@+id/tv_click_set"
            android:background="@drawable/buy_all_corner_kong"/>

    </LinearLayout>

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/number_winning_num"
            android:textColor="@color/trade_text_color6"
            android:paddingLeft="10dp"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="0.6"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv_winning_number"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_color_black"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="1"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/number_winning_tprice"
            android:textColor="@color/trade_text_color6"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="0.6"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv_winning_count"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_color_black"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/number_winning_tprice"
            android:textColor="@color/trade_text_color6"
            android:paddingLeft="10dp"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="0.6"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv_winning_price"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_color_black"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="1"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/number_winning_tcount"
            android:textColor="@color/trade_text_color6"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="0.6"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv_winning_tcount"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_color_black"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="1" />

    </LinearLayout>

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/number_winning_tmoney"
            android:textColor="@color/trade_text_color6"
            android:paddingLeft="10dp"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="0.6"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv_winning_money"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_color_black"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="1" />

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/number_winning_money"
            android:textColor="@color/trade_text_color6"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="0.6"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_color_black"
            android:paddingTop="1dp"
            android:paddingBottom="1dp"
            android:layout_weight="1"
            android:id="@+id/tv_winning_cash"/>

    </LinearLayout>

    <View android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/trade_divide_line"/>

</LinearLayout>