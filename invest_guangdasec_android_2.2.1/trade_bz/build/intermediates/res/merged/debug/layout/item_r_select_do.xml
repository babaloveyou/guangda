<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/trade_color_white">

    <View android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/trade_normal_background"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_marginLeft="5dp"
            android:hint="@string/no_text_set"
            android:id="@+id/tv_r_do_bs"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_marginLeft="5dp"
            android:hint="@string/no_text_set"
            android:id="@+id/tv_r_do_date"/>

    </LinearLayout>

    <View android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/trade_divide_line"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_trade_count"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_trade_num"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not3"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_trade_price"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_trade_money"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_trade_balances"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not4"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_qing_money"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not5"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_fare_shou"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not6"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_fare_qing"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not7"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_fare_trade"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not8"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_fare_yin"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not9"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_fare_guo_hu"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not10"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_compact_id"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not11"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_trade_id"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not12"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_stock_yu"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not13"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_money_yu"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not14"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_stock_account"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:text="@string/contract_not15"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2.6"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_market"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:text="@string/contract_not16"
            android:visibility="gone"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_do_remark"
            android:visibility="gone"/>

    </LinearLayout>

</LinearLayout>