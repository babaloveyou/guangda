<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/trade_color_white"
    android:orientation="vertical">

    <View android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/trade_normal_background"/>

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_stock_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/hk_select_left_distances"
            android:hint="@string/no_text_set"
            android:paddingBottom="2dp"
            android:paddingTop="4dp"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_title"
            />
        <TextView
            android:id="@+id/tv_stock_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textSize="@dimen/hk_select_title"
            />
    </LinearLayout>

    <View android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/trade_divide_line"/>


    <!--第一栏-->
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/hk_select_left_distances"
            android:layout_marginTop="@dimen/hk_select_item_height"
            android:layout_weight="0.6"
            android:text="@string/hk_statement_item0"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:id="@+id/tv_uddelegere_priser"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_item_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_item_height"
            android:layout_weight="0.6"
            android:text="@string/hk_statement_item1"
            android:textSize="@dimen/hk_select_item"/>
        <TextView android:id="@+id/tv_business_price"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/hk_select_left_distances"
            android:layout_marginTop="@dimen/hk_select_item_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>
    </LinearLayout>
    <!--第一栏-->
    <LinearLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/hk_select_left_distances"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
            android:text="@string/hk_statement_item2"
            android:textSize="@dimen/hk_select_item"/>

        <TextView android:id="@+id/tv_uddelegere_numbler"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="1"
            android:hint="@string/no_text_set"
            android:textColor="@color/trade_text_color6"
            android:textSize="@dimen/hk_select_item"/>


        <TextView android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/hk_select_height"
            android:layout_weight="0.6"
