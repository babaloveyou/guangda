<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/trade_color_white">

    <View android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/trade_normal_background"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:hint="@string/no_text_set"
            android:id="@+id/tv_r_not_name"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_marginLeft="5dp"
            android:hint="@string/no_text_set"
            android:id="@+id/tv_r_not_code"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:text="@string/contract_not_create_time"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:layout_marginLeft="5dp"
            android:hint="@string/no_text_set"
            android:id="@+id/tv_r_not_date"/>

    </LinearLayout>

    <View android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/trade_divide_line"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not0"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_bs"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_status"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_status"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_trade_count"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_trade_num"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_trade_money"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_trade_balances"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_rong_money"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_r_money"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_fit"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_yk"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_nrtn_money"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_money"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_cash"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_cash"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_yrtn_li"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_yes_li"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_not_return"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_not_li"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_fa_li"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_yes_fa"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:text="@string/contract_not_fa_li2"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_not_fa"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:text="@string/contract_not_all_time"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_cut_date"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:text="@string/contract_not1"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:hint="@string/no_text_set"
            android:singleLine="true"
            android:textColor="@color/trade_text_color6"
            android:id="@+id/tv_r_not_id"/>

    </LinearLayout>
</LinearLayout>