<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/trade_normal_background">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_monetary_fund_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:background="@color/transparent"
                android:textColor="@color/trade_text_color6"
                android:id="@+id/tv_in_monetary_fund"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_monetary_fund_subscription_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:visibility="gone"
                android:id="@+id/tv_in_monetary_fund_subscription"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_monetary_fund_purchase_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:id="@+id/tv_in_monetary_fund_purchase"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_monetary_fund_redemption_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:id="@+id/tv_in_monetary_fund_redemption"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_lof_fund_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:background="@color/transparent"
                android:textColor="@color/trade_text_color6"
                android:id="@+id/tv_in_lof_fund"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_fund_subscription_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:id="@+id/tv_in_fund_subscription"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_fund_purchase_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:id="@+id/tv_in_fund_purchase"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_fund_redemption_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:id="@+id/tv_in_fund_redemption"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_entrust_inquire_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:background="@color/transparent"
                android:textColor="@color/trade_text_color6"
                android:visibility="gone"
                android:id="@+id/tv_in_entrust_inquire"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"
                android:visibility="gone"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_cur_day_entrust_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:visibility="gone"
                android:id="@+id/tv_in_cur_day_entrust"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"
                android:visibility="gone"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_cur_day_transcations_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:visibility="gone"
                android:id="@+id/tv_in_cur_day_transcations"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"
                android:visibility="gone"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_history_entrust_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:visibility="gone"
                android:id="@+id/tv_in_history_entrust"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"
                android:visibility="gone"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/in_history_transcations_title"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="20dp"
                android:paddingRight="8dp"
                android:background="@color/trade_color_white"
                android:textColor="@color/trade_text_color6"
                android:drawableRight="@drawable/arrows_right_gray"
                android:visibility="gone"
                android:id="@+id/tv_in_history_transcations"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/trade_divide_line"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>